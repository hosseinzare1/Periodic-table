Sub sizef
	
	PNameFA.Text="ژرمانیوم"
	PAtomicNumber.Text="999"

	ChangeFont.FitToLabelSize(PNameFA)
	ChangeFont.FitToLabelSize(PAtomicNumber)

	sizefT=PNameFA.TextSize
	sizefN= PAtomicNumber.TextSize-9
	
