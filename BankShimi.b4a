Sub Activity_Create(FirstTime As Boolean)
	
	
	

	AtomicinfoEN = Array As String("Exact AtomicWeight","electronegativity (Pauling)","1st ionization potential (kJ/mol)","2nd ionization potential (kJ/mol)","3rd ionization potential (kJ/mol)", _
	"Electron Affinity (kJ/mol)","common oxidation states","ions commonly formed", _
	"atomic radius (Å) ","ionic radius (Å) ","covalent radius (Å) ","polarizability(Å^3)")
	
	PhysicalinfoEN = Array As String("density g/mL","atomic volume (cm^3/mol)","crystal structure","electrical conductivity (mho/cm) ","hardness(mohs)","color")
	
	TherminfoEN = Array As String("Melting Point, °C","Boiling Point, °C","State at Room Temp","specific heat (J/g K)","heat of fusion (kJ/mol)","heat of vaporization (kJ/mol)","thermal conductivity (W/(m K)) ","heat atomization(kJ/mol)")
	
	ReactioninfoEN = Array As String("reaction with air","reaction with water","reaction with 6M HCl","reaction with 15M HNO3","reaction with 6M NaOH","hydride(s)","oxide(s)","chloride(s)")
	
	OtherinfoEN = Array As String("CAS Registry No","mg/kg in Earths crust","mg/L in seawater","% human body mass","year discovered","toxic?","carcinogenic?")
	
	'Do not forget to load the layout file created with the visual designer. For example:
	Activity.LoadLayout("PeriodicLayout")
	
	fristT=True
	fristT2=True
	
'	pushpole.initialize()
	
	InfoSlid.Initialize("InfoSlid")
	InfoSlid.Menu.LoadLayout("main")
	InfoSlid.BehindOffset=50%x
	InfoSlid.Mode=InfoSlid.RIGHT
	sizef
	
	ChangeFont.FitToLabelSize(ExitTool)
	ChangeFont.FitToLabelSize(AboutTool)
	ChangeFont.FitToLabelSize(InfoSlidBut)
	
	
	DKhoroj.Initialize("dkhoroj")
	DKhoroj.Title("خروج").content("آیا میخواهید از برنامه خارج شوید؟")
	DKhoroj.positiveText("بله").negativeText("خیر")
'	DKhoroj.positiveColor(Mcolor.md_yellow_500).neutralColor(Mcolor.md_green_500)
'	DKhoroj.canceledOnTouchOutside(True)
	
	
	InfoHelpDialog.Initialize("InfoHelpDialog")
