	AddTitr("مشخصات ترمودینامیکی",Mcolor.md_cyan_600)
	
	For i = 0 To 7
		MultiPanel2.Initialize("p")
		If cursor1.GetString(TherminfoEN(i))<>Null And cursor1.GetString(TherminfoEN(i))<>"" Then
		
			SVlist.Panel.AddView(MultiPanel2,(0.01*xS)    ,offsety,  48%x ,  (0.1*yS))
			offsety=offsety+0.107*yS
