	InfoHelpDialog.positiveText("بستن").negativeText("دیگر نشان نده.")
	InfoHelpDialog.canceledOnTouchOutside(True)
	
	InfoHelpDialog2.Initialize("InfoHelpDialog2")
	InfoHelpDialog2.Title("راهنما").content("با انتخاب هریک از عناصر میتوانید مشخصات کامل آن را مطالعه کنید.")
	InfoHelpDialog2.positiveText("بستن").negativeText("دیگر نشان نده.")
	InfoHelpDialog2.canceledOnTouchOutside(True)
'	اینه
	If File.Exists(File.DirInternal,"ph.log")= False Then
		File.Copy(File.DirAssets,"photoname.png",File.DirInternal,"ph.log")
		arc.DecryptZipWithString("hossein@1381@zu")
		arc.UnZip(File.Combine(File.DirInternal,"ph.log"), File.DirInternal,"Archiver")
	End If
' اینه
	CDialog.Initialize("CDialog")
	CDialog.title("بانک شیمی")
	CDialog.content("کاری از حسین زرعی بارنجی "&CRLF&"ایمیل:Hosseiny625@gmail.com")

	CDialog.positiveText("بستن")
	
	InfoDialog.Initialize("InfoDialog")

	sql1.Initialize(File.DirInternal,"picname.hck", True)
	cursor1=sql1.ExecQuery("SELECT * FROM Periodic")
	

	InfoSql.Initialize(File.DirInternal,"info.hck", True)
	InfoCursor=InfoSql.ExecQuery("SELECT * FROM InfoTabel")
	InfoCursor.Position=0
	
'	
'	arrEN = Array As String("AtomicWeight","Melting Point, °C","Boiling Point, °C","common oxidation states","ions commonly formed","ar")
'	arrFA = Array As String("جرم اتمی","نقطه ذوب","نقطه جوش","حالت های عدد اکسایش","یون ها","آرایش الکترونی")
'	
	width1=0.05*xS
	Height1=0.09*yS
	
	widthHelp=0.065*xS
	HeightHelp=0.08*yS
	
	For i = 1 To 120
		cursor1.Position=i-1
	
		Group=cursor1.GetString("GroupP")
		Period=(cursor1.GetString("PeriodP"))
			
		Left1=((Group-1)*width1)+(Group*0.004*xS)+(0.01*xS)
		Top1=((Period-1)*Height1)+(Period*0.004*yS)+(0.11*yS)
