			offsety=offsety+0.107*yS
			MultiPanel2.LoadLayout("ElementInfoPanel")
			PanelInfo.Color=Mcolor.md_blue_400
		
			InfoCursor=InfoSql.ExecQuery("SELECT * FROM InfoTabel WHERE English LIKE '"&AtomicinfoEN(i)&"%"&"'")
			InfoCursor.Position=0
		
			LabelT.Text=InfoCursor.GetString("Persian")&":"
	
			LabelC.Text=cursor1.GetString(AtomicinfoEN(i))
			
			InfoBut.Tag=AtomicinfoEN(i)
			
		End If
		
	Next
	
	AddTitr("واکنش های شیمیایی",Mcolor.md_green_600)
	For i = 0 To 7
		If cursor1.GetString(ReactioninfoEN(i))<>Null And cursor1.GetString(ReactioninfoEN(i))<>"" Then
		
