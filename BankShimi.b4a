			Case  "6"
				PElementPanel.Color=Mcolor.md_orange_400
			Case "7"
				PElementPanel.Color=Mcolor.md_purple_400
			Case "8"
				PElementPanel.Color=Mcolor.md_yellow_400
			Case "9"
				PElementPanel.Color=Mcolor.md_teal_400
			Case "10"
				PElementPanel.Color=Mcolor.md_lime_500
		End Select
	
	Next


	
	For i = 121 To 130
		cursor1.Position=i-1
	
		Group=cursor1.GetString("GroupP")
		Period=(cursor1.GetString("PeriodP"))
			
		Left1=((Group-1)*widthHelp)+(Group*0.004*xS)+(0.01*xS)
		Top1=((Period-1)*HeightHelp)+(Period*0.004*yS)+(0.11*yS)
'		+(0.1*xS)
		MultiPanel.Initialize("MultiPanel")
		
		PeriodicPanle.AddView(MultiPanel,Left1 ,Top1,widthHelp,HeightHelp)
			
		MultiPanel.LoadLayout("ElementHelp")

		HelpLabel.Text=cursor1.GetString("PersianElementName")
	
		HelpLabel.TextSize=sizefT-10
		'ChangeFont.FitToLabelSize(HelpLabel)
		
		
		Select cursor1.GetString("TypeB")
			Case "1"
				HelpPanel.Color=Mcolor.md_deep_purple_400
			Case "2"
				HelpPanel.Color=Mcolor.md_pink_400
			Case "3"
				HelpPanel.Color=Mcolor.md_cyan_400
			Case "4"
				HelpPanel.Color=Mcolor.md_blue_400
			Case  "5"
				HelpPanel.Color=Mcolor.md_deep_orange_400
			Case  "6"
				HelpPanel.Color=Mcolor.md_orange_400
			Case "7"
				HelpPanel.Color=Mcolor.md_purple_400
			Case "8"
				HelpPanel.Color=Mcolor.md_yellow_400
			Case "9"
				HelpPanel.Color=Mcolor.md_teal_400
			Case "10"
				HelpPanel.Color=Mcolor.md_lime_500
		End Select
	
	Next

	InfoCursor=InfoSql.ExecQuery("SELECT * FROM InfoTabel WHERE English = 'close2'")
	InfoCursor.Position=0
	If fristT2=True And InfoCursor.GetString("Info")="false" Then
		InfoHelpDialog2.show
		fristT2=False
	End If

	InfoCursor=InfoSql.ExecQuery("SELECT * FROM InfoTabel")
	InfoCursor.Position=0

	lim.SetLengthFilter(PSymbol,2)
	lim.SetLengthFilter(PAtomicNumber,3)
	lim.SetCustomFilter(PNameEN,PNameEN.INPUT_TYPE_TEXT ,"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz")
	lim.SetCustomFilter(PSymbol,PSymbol.INPUT_TYPE_TEXT ,"ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz")
