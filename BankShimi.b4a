Sub AddTitr(Titr As String , color As Int)
	
	
	
	
	If Titr<>"مشخصات فیزیکی" Then
		offsety=offsety+0.05*yS
	End If
	MultiPanel2.Initialize("p")
	SVlist.Panel.AddView(MultiPanel2,(0.01*xS)    ,offsety,  48%x ,  (0.1*yS))
	offsety=offsety+0.1*yS
	MultiPanel2.LoadLayout("InfoTitr")
	PanelTitr.color=color

	InfoTitr.Text=Titr
	InfoTitr.TextSize=sizefT
End Sub

Sub data
	InfoHelpLabel.Visible=False
	
	SVlist.Panel.RemoveAllViews
	offsety=0.03*yS
	
	AddTitr("مشخصات فیزیکی",Mcolor.md_lime_500)
	
	For i = 0 To 5
		MultiPanel2.Initialize("p")
		If cursor1.GetString(PhysicalinfoEN(i))<>Null And cursor1.GetString(PhysicalinfoEN(i))<>"" Then
			SVlist.Panel.AddView(MultiPanel2,(0.01*xS)    ,offsety,  48%x ,  (0.1*yS))
			offsety=offsety+0.107*yS
			MultiPanel2.LoadLayout("ElementInfoPanel")
			PanelInfo.Color=Mcolor.md_lime_400
			
			InfoCursor=InfoSql.ExecQuery("SELECT * FROM InfoTabel WHERE English LIKE '"&PhysicalinfoEN(i)&"%"&"'")
			InfoCursor.Position=0
			
			LabelT.Text=InfoCursor.GetString("Persian")&":"
			LabelC.Text=cursor1.GetString(PhysicalinfoEN(i))
			
			InfoBut.Tag=PhysicalinfoEN(i)
			
		End If
		
	Next
	
	AddTitr("مشخصات اتمی",Mcolor.md_blue_600)
	
	
	For i = 0 To 11
		MultiPanel2.Initialize("p")
