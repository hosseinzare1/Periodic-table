		PeriodicPanle.AddView(MultiPanel,Left1 ,Top1,width1,Height1)
			
		MultiPanel.LoadLayout("ElementLayout")

		PElementSymbol.Text=cursor1.GetString("AtomicSymbol")
		PElementName.Text=cursor1.GetString("PersianElementName")
		ElementBut.Tag=cursor1.GetString("AtomicNumber")
		
		
		PElementName.TextSize=sizefT-26
		PElementSymbol.TextSize=sizefT-5
		PElementNumber.TextSize=sizefT-20
		InfoHelpLabel.TextSize=sizefT
		
		
